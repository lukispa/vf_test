FUNCTION "LSPARVI"."private.lucas::SF_TEST_FP" ( IN IN_PERIOD NVARCHAR( 1 ) ) 
	RETURNS OUT_DATE_FROM NVARCHAR( 10 )
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER AS
BEGIN

	OUT_DATE_FROM := CASE WHEN :IN_PERIOD = '' THEN ''
							WHEN :IN_PERIOD = 1 THEN '2019-01-01'
							WHEN :IN_PERIOD = 2 THEN '2020-01-01'
							ELSE ''
						END;
		
END;